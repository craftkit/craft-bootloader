{
    "tags": {
        "allowUnknownTags": true
    },
	"plugins": ["plugins/markdown"],
	"markdown": {
		"parser": "gfm",
		"hardwrap": true
	},
    "docdash": {
    	"appname" : "CraftBootloader",
        "static": true,
        "sort": false,
        "sectionOrder": [
             "Classes",
             "Namespaces",
             "Modules",
             "Externals",
             "Events",
             "Mixins",
             "Tutorials",
             "Interfaces"
        ],
        "disqus": "",
        "openGraph": {
            "title": "",
            "type": "website",
            "image": "",
            "site_name": "",
            "url": ""
        },
        "meta": {
            "title": "",
            "description": "",
            "keyword": ""
        },
        "search": true,
        "collapse": true,
        "wrap": true,
        "typedefs": true,
        "navLevel": false,
        "private": false,
        "removeQuotes": "none",
        "scripts": []
    }
}
